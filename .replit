run = "npm run build && vite preview"
entrypoint = "main.js"

hidden = [".config", ".gitignore"]
modules = ["nodejs-22"]

[nix]
channel = "stable-24_11"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", ".config"]

[deployment]
deploymentTarget = "static"
build = ["npm", "run", "build"]
publicDir = "dist"

[[ports]]
localPort = 3001
externalPort = 3002

[[ports]]
localPort = 4173
externalPort = 3001

[[ports]]
localPort = 5173
externalPort = 80

[[ports]]
localPort = 5174
externalPort = 3000
